.head-view {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.head {
  padding: 40rpx 0 0 0;
  text-align: center;
  align-self: center;
  color: #07C160;
}

.search {
  display: flex;
  flex-direction: row;
  border: 5rpx solid #07C160;
  border-radius: 80rpx;
  padding: 10rpx;
  align-items: center;
  margin-top: 30rpx;
  font-size: 33rpx;
}

.sort {
  font-size: 30rpx;
  margin-top: 40rpx;
}

.sort-view {
  width: 96%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.sort_market {
  border-style: solid;
  border-color: #07C160;
  border-width: 5rpx 2.5rpx 5rpx 5rpx;
  padding: 10rpx;
}

.sort_study {
  border-style: solid;
  border-color: #07C160;
  border-width: 5rpx 5rpx 5rpx 2.5rpx;
  padding: 10rpx;
}

.sort_apartment {
  border-style: solid;
  border-color: #07C160;
  border-width: 5rpx 5rpx 5rpx 2.5rpx;
  padding: 10rpx;
}

.content-view {
  width: 100%;
  height: 100%;
}

.content {
  width: 100%;
  margin-top: -60rpx;
}

.content-item {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 300rpx;
  margin-top: 73rpx;
}

.not-radio {
  width: 85%;
}

.header_out {
  width: 100%;
}

.header {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 35rpx;
  padding: 10rpx 40rpx 10rpx 40rpx;
  border-left: 6rpx #07C160 solid;
  margin: 20rpx 0 15rpx 0;
  align-items: center;
}

.item-content-one {
  padding: 10rpx 40rpx 10rpx 40rpx;
  margin: 20rpx 0 15rpx 0;
  align-items: center;
}

.item-content-one .dot {
  font-weight: bold;
  color: #07C160;
}

.item-content-one .activity {
  margin-left: 20rpx;
}

.radio {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 15%;
  height: 40%;
  margin-left: 20rpx;
}

radio {
  align-items: center;
}

.item-img-view {
  height: 430rpx;
}

.item-img {
  width: 100%;
  height: 100%;
}

.item-text-view {
  font-size: 35rpx;
  padding: 10rpx 40rpx 5rpx 40rpx;
  margin: 10rpx 0 5rpx 0;
}

.item-text2-view {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  color: #4d4d4d;
  font-size: 30rpx;
  align-items: center;
}

.item-text2-view text {
  color: #07C160;
  font-size: 35rpx;
}

.line {
  background-color: #d6d6d6;
  height: 8rpx;
}

button {
  margin-top: 120rpx;
  margin-bottom: 30rpx;
}